{
  "name": "pug-cli",
  "version": "0.1.1",
  "description": "Pug's CLI interface",
  "bin": {
    "pug": "./index.js"
  },
  "preferGlobal": true,
  "keywords": [],
  "dependencies": {
    "chalk": "^1.0.0",
    "commander": "^2.8.1",
    "escape-string-regexp": "^1.0.3",
    "pug": "^2.0.0-alpha1",
    "mkdirp": "^0.5.1"
  },
  "devDependencies": {
    "istanbul": "*",
    "mocha": "*",
    "rimraf": "^2.3.4"
  },
  "scripts": {
    "test": "mocha -R spec --bail",
    "precoverage": "rimraf coverage && rimraf cov-pt*",
